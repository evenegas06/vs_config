{
    "workbench.iconTheme": "vscode-icons",
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "terminal.integrated.defaultProfile.windows": "Git Bash",
    "terminal.integrated.cursorBlinking": true,
    "workbench.colorTheme": "Palenight (Mild Contrast)",
    "git.ignoreMissingGitWarning": true,
    "workbench.editorAssociations": {
        "*.php": "default"
    },
    "editor.guides.bracketPairs": "active",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairsHorizontal": true,
    "workbench.colorCustomizations": {
        "editorBracketHighlight.foreground1": "#ffd700",
        "editorBracketHighlight.foreground2": "#da70d6",
        "editorBracketHighlight.foreground3": "#87cefa",
        "editorBracketHighlight.unexpectedBracket.foreground": "#ff0000",
        "editorCursor.background": "#fff",
        "editorCursor.foreground": "#fff"
    },
    "editor.tokenColorCustomizations": {
        "[Palenight Italic]": {
            "textMateRules": [
                {
                    "scope": [
                        "entity.name.tag support.class.component",
                        "meta.tag.other.html",
                        "meta.tag.other.js",
                        "meta.tag.other.tsx",
                        "entity.name.tag.tsx",
                        "entity.name.tag.js",
                        "entity.name.tag",
                        "meta.tag.js",
                        "meta.tag.tsx",
                        "meta.tag.html",
                        // "entity.name.tag.css",
                        "constant.language.boolean.false.js",
                        "constant.language.boolean.true.js.jsx",
                        "variable.language.this.php",
                        "constant.language.json.comments",
                        "constant.numeric.decimal.php",
                        "keyword.operator.increment-decrement.php",
                        "constant.numeric.decimal.js.jsx",
                        "constant.language.null.js.jsx",
                        "constant.numeric.decimal.js",
                        "meta.structure.dictionary.json meta.structure.dictionary.value constant.language",
                        "constant.numeric",
                        "constant.language.boolean",
                        "constant.language.null.js"
                    ],
                    "settings": {
                        "foreground": "#f8b3c0"
                    }
                },
                {
                    "scope": [
                        "meta.object-literal.key.js.jsx",
                        "meta.object-literal.key.js"
                    ],
                    "settings": {
                        "foreground": "#80CBC4"
                        //"foreground": "#72E6E8",
                    }
                },
                {
                    "scope": [
                        "variable.parameter.js"
                    ],
                    "settings": {
                        "foreground": "#BFC7D5"
                        // "foreground": "#46bdd8",
                    }
                },
                {
                    "scope": [
                        "entity.name.tag support.class.component"
                    ],
                    "settings": {
                        "foreground": "#4be6eb"
                        //"foreground": "#72E6E8",
                    }
                },
                {
                    "scope": [
                        "entity.other.attribute-name"
                    ],
                    "settings": {
                        "foreground": "#e8cf8a"
                        //"foreground": "#72E6E8",
                    }
                }
            ]
        },
        "[Palenight (Mild Contrast)]": {
            "textMateRules": [
                {
                    "scope": [
                        "entity.name.tag support.class.component",
                        "meta.tag.other.html",
                        "meta.tag.other.js",
                        "meta.tag.other.tsx",
                        "entity.name.tag.tsx",
                        "entity.name.tag.js",
                        "entity.name.tag",
                        "meta.tag.js",
                        "meta.tag.tsx",
                        "meta.tag.html",
                        // "entity.name.tag.css",
                        "constant.language.boolean.false.js",
                        "constant.language.boolean.true.js.jsx",
                        "variable.language.this.php",
                        "constant.language.json.comments",
                        "constant.numeric.decimal.php",
                        "keyword.operator.increment-decrement.php",
                        "constant.numeric.decimal.js.jsx",
                        "constant.language.null.js.jsx",
                        "constant.numeric.decimal.js",
                        "meta.structure.dictionary.json meta.structure.dictionary.value constant.language",
                        "constant.numeric",
                        "constant.language.boolean",
                        "constant.language.null.js"
                    ],
                    "settings": {
                        "foreground": "#FFB3ED"
                        // "foreground": "#f8b3c0"
                    }
                },
                {
                    "scope": [
                        "meta.object-literal.key.js.jsx",
                        "meta.object-literal.key.js"
                    ],
                    "settings": {
                        "foreground": "#72E6E8"
                    }
                },
                {
                    "scope": [
                        "variable.parameter.js"
                    ],
                    "settings": {
                        "foreground": "#BFC7D5"
                    }
                },
                {
                    "scope": [
                        "entity.name.tag support.class.component"
                    ],
                    "settings": {
                        "foreground": "#ffcd76"
                        // "foreground": "#ffdea4"
                        // "foreground": "#EBD3A9"
                        //"foreground": "#72E6E8",
                    }
                },
                {
                    "scope": [
                        "entity.other.attribute-name",
                        "entity.name.tag.css"
                    ],
                    "settings": {
                        // "foreground": "#97cbe2"
                        // "foreground": "#afef89"
                        // "foreground": "#a3ff6d"
                        "foreground": "#afa0f3"
                        //"foreground": "#72E6E8",
                    }
                },
                {
                    "scope": [
                        "support.type.property-name"
                    ],
                    "settings": {
                        "foreground": "#cb9fe9"
                        //"foreground": "#72E6E8",
                    }
                }
            ]
        }
    },
    "cSpell.language": "en,es-ES",
    "scm.inputFontSize": 14,
    "editor.fontSize": 14,
    "terminal.integrated.fontSize": 12,
    "intelephense.format.braces": "k&r",
    "cSpell.userWords": [
        "Laravel",
        "livewire",
        "Livewire",
        "Venegas"
    ],
    "editor.linkedEditing": true,
    "editor.cursorSmoothCaretAnimation": "off",
    "editor.cursorBlinking": "expand",
    "editor.fontLigatures": "'ss02'",
    "editor.fontWeight": "400",
    "editor.fontFamily": "'Fira Code', Consolas, 'Courier New', monospace",
    "javascript.format.semicolons": "insert",
    "editor.unicodeHighlight.includeComments": false,
    "editor.unicodeHighlight.includeStrings": false,
    "[javascript]": {
        "editor.defaultFormatter": "vscode.typescript-language-features"
    },
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.currentLine.enabled": false,
    "gitlens.codeLens.recentChange.enabled": false,
    "gitlens.codeLens.authors.enabled": false,
    "gitlens.codeLens.scopes": [],
    "gitlens.hovers.enabled": false,
    "reactSnippets.settings.importReactOnTop": false,
    "[blade]": {
        "editor.defaultFormatter": "shufo.vscode-blade-formatter"
    },
    "window.zoomLevel": 1.8,
    "editor.glyphMargin": false,
    // "breadcrumbs.enabled": false,
    "terminal.integrated.env.windows": {},
    "console-ninja.showDateTime": false,
    "console-ninja.captureFunctions": true,
    "bladeFormatter.format.wrapAttributes": "force-expand-multiline"
}